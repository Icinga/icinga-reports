    <query>
        <localResource
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            exportedWithPermissions="false" xsi:type="query">
            <folder>/icinga/inputcontrols/p_contact_object_id_files</folder>
            <name>Query</name>
            <version>0</version>
            <label>Query</label>
            <creationDate>2017-08-04T00:50:46.193Z</creationDate>
            <updateDate>2017-08-04T00:50:46.193Z</updateDate>
            <language>sql</language>
            <queryString>select a.contact_object_id as p_contact_object_id, a.alias 
from icinga_contacts a 
join icinga_objects b on b.object_id=a.contact_object_id 
where b.is_active=1 
order by alias</queryString>
